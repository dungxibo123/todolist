<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="255.0" prefWidth="456.0" styleClass="pane" stylesheets="@../Design/Main.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddEventWindow.Controller"
            fx:id="anchorPane">
   <children>
      <TextField layoutX="97.0" layoutY="30.0" prefHeight="31.0" prefWidth="354.0" fx:id="tfActivity"/>
      <Label alignment="CENTER" layoutX="14.0" layoutY="30.0" prefHeight="31.0" prefWidth="89.0" styleClass="lb" stylesheets="@../Design/Main.css" text="Activity" textAlignment="CENTER" textFill="WHITE" wrapText="true" />
      <ComboBox layoutX="62.0" layoutY="112.0" prefHeight="34.0" prefWidth="100.0" promptText="Ngày" styleClass="cb" stylesheets="@../Design/Main.css" />
      <ComboBox layoutX="168.0" layoutY="112.0" prefHeight="31.0" prefWidth="107.0" promptText="Tháng" styleClass="cb" stylesheets="@../Design/Main.css" />
      <ComboBox layoutX="282.0" layoutY="112.0" prefHeight="31.0" prefWidth="100.0" promptText="Năm" styleClass="cb" stylesheets="@../Design/Main.css" />
      <ComboBox layoutX="124.0" layoutY="157.0" prefHeight="31.0" prefWidth="89.0" promptText="Giờ" styleClass="cb" stylesheets="@../Design/Main.css" />
      <ComboBox layoutX="231.0" layoutY="157.0" prefHeight="34.0" prefWidth="100.0" promptText="Phút" styleClass="cb" stylesheets="@../Design/Main.css" />
      <ButtonBar layoutX="253.0" layoutY="208.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" text="Add" fx:id="addButton" onAction="#addActivity"/>
            <Button mnemonicParsing="false" text="Cancel" fx:id="cancelButton" onAction="#cancel"/>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
